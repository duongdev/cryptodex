{
  "name": "cryptodex",
  "version": "0.0.0",
  "private": "true",
  "module": "index.ts",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "cms:build": "yarn workspace @cryptodex/cms run build",
    "cms:dev": "yarn workspace @cryptodex/cms run dev",
    "cms:start": "yarn workspace @cryptodex/cms run start",
    "web:dev": "yarn workspace @cryptodex/web run dev",
    "clean": "rm -rf node_modules {packages,apps}/*/node_modules yarn.lock {packages,apps}/*/yarn.lock"
  },
  "devDependencies": {
    "@cryptodex/eslint-config": "0.0.1",
    "ajv": "^8.13.0",
    "typescript": "5"
  },
  "dependencies": {
    "turbo": "^1.13.3"
  },
  "overrides": {
    "prettier": "^3.2.5"
  },
  "trustedDependencies": [
    "@vercel/speed-insights",
    "core-js-pure"
  ]
}